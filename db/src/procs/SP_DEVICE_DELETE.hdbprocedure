PROCEDURE "wavepress.db.procs::SP_DEVICE_DELETE"( IN IN_DEVICE_ID INT, OUT IS_DELETED INT)
   LANGUAGE SQLSCRIPT
   SQL SECURITY INVOKER
   --DEFAULT SCHEMA <default_schema_name>
   -- READS SQL DATA 
   AS
BEGIN
   
   DELETE FROM "wavepress.db.syn::DEVICES"
   WHERE "ID" = :IN_DEVICE_ID;
   COMMIT;
   
   SELECT CASE WHEN COUNT(*) = 0 THEN 1 ELSE 0 END INTO IS_DELETED 
   FROM "wavepress.db.syn::DEVICES"
   WHERE "ID" = :IN_DEVICE_ID;
   
END